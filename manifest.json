{
  "manifest_version": 2,
  "name": "chrome bookmark helper",
  "version": "1.0.0",
  "icons": {
    "128": "./assets/icon.png"
  },
  "browser_action": {
    "default_icon": "./assets/icon.png",
    "default_popup": "./popup/index.html"
  },
  "permissions": ["bookmarks"],
  "background": {
    "scripts": ["./background/index.js"]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+B"
      },
      "description": "show popup"
    }
  }
}